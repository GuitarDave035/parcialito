{
    "dev": {
        "app_function": "lambda1.lambda_handler",
        "aws_region": "us-east-1",
        //"profile_name": "default",
        "project_name": "zappa-pagina",
        "runtime": "python3.10",
        "keep_warm": false,
        "s3_bucket": "zappa-myzdcsluq",
        "apigateway_enabled": false,
        "manage_roles": false,
        "role_name": "LabRole",
        "exclude": [
            "venv",
            "parcialito",
            "tests",
            "__pycache__",
            "node_modules",
            "*.pyc",
            "*.pyo",
            "*.txt"
        ],
        "events": [
            {
                "function": "lambda1.lambda_handler",
                "expression": "cron(0 9 ? * MON *)"
            }
        ]

    }
}